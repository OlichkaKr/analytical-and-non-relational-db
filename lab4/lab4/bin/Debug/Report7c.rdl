    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@BODY">
            <Value>=Parameters!BODY.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT Comment.body, [User].first_name, [User].last_name, [User].public_display_name, [User].about_me, [User].profile_link, [User].email, [User].password
FROM     Comment INNER JOIN
                  Popularity ON Comment.id = Popularity.comment_id INNER JOIN
                  [User] ON Popularity.user_id = [User].id
WHERE Comment.body = @BODY</CommandText>
      </Query>
